{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.rulers": [
    {
      "column": 100
    }
  ],
  "workbench.colorCustomizations": {
    "activityBar.background": "#29264F",
    "titleBar.activeBackground": "#4b9c6d",
    "titleBar.activeForeground": "#29264F",
    "titleBar.border": "#29264F"
  },
  "workbench.colorTheme": "Monokai"
}
