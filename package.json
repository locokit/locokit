{
  "name": "locokit",
  "version": "0.6.0-alpha.0",
  "description": "Low-Code Kit platform",
  "workspaces": [
    "packages/**",
    "docs",
    "front",
    "api"
  ],
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "serve:front": "npm run serve --workspace=front",
    "serve:api": "npm run dev --workspace=api",
    "docs:start": "DEBUG=true vitepress dev docs",
    "docs:build": "BASE_URL=/docs vitepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.makina-corpus.net:lck/locokit.git"
  },
  "author": "Mathieu Dartigues",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "vitepress": "^0.20.0"
  }
}
