    "@locokit/sdk": "workspace:1.0.0-alpha.0",
    "@locokit/vue-components": "workspace:^",
    "@primevue/forms": "^4.2.4",
    "@primevue/themes": "^4.2.4",
    "bootstrap-icons": "^1.11.3",
    "pinia": "^2.2.8",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.4",
    "tailwindcss": "^3.4.15",
    "tailwindcss-primeui": "^0.3.4",
    "unplugin-vue-router": "^0.10.8",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.5",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
